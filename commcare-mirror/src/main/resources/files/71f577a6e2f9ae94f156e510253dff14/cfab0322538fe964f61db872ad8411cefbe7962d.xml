<h:html xmlns:h="http://www.w3.org/1999/xhtml" xmlns:orx="http://openrosa.org/jr/xforms" xmlns="http://www.w3.org/2002/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
	<h:head>
		<h:title>Update Referral Form</h:title>
		<model>
			<instance>
				<data xmlns:jrm="http://dev.commcarehq.org/jr/xforms" xmlns="http://openrosa.org/formdesigner/B64DD00D-B133-4768-B6DB-4C2B4C1F34F1" uiVersion="1" version="1" name="Update Referral Form">
					<not_availed_reasons>
						<new_diagnosis_na_reason/>
						<end-of-life_hospice_na_reason/>
						<hiv_counseling_na_reason/>
						<family_planning_na_reason/>
						<sexually_transmitted_na_reason/>
						<art_adherence_na_reason/>
						<pain_management_na_reason/>
						<pmtct_na_reason/>
						<condoms_na_reason/>
						<other_health_service_na_reason/>
					</not_availed_reasons>
					<form_type>update_referral</form_type>
					<pain_management/>
					<end_of_life_hospice/>
					<sexually_transmitted_infection/>
					<art_adherence_counseling/>
					<pmtct/>
					<hiv_counseling/>
					<family_planning/>
					<other_health_services/>
					<condoms/>
					<new_diagnosis/>
				</data>
			</instance>
			<bind nodeset="/data/not_availed_reasons"/>
			<bind nodeset="/data/not_availed_reasons/new_diagnosis_na_reason" type="xsd:string" relevant="/data/new_diagnosis = 'Not Availed' or /data/new_diagnosis = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/end-of-life_hospice_na_reason" type="xsd:string" relevant="/data/end_of_life_hospice = 'Not Availed' or /data/end_of_life_hospice = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/hiv_counseling_na_reason" type="xsd:string" relevant="/data/hiv_counseling = 'Not Availed' or /data/hiv_counseling = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/family_planning_na_reason" type="xsd:string" relevant="/data/family_planning = 'Not Availed' or /data/family_planning = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/sexually_transmitted_na_reason" type="xsd:string" relevant="/data/sexually_transmitted_infection = 'Not Availed' or /data/sexually_transmitted_infection = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/art_adherence_na_reason" type="xsd:string" relevant="/data/art_adherence_counseling = 'Not Availed' or /data/art_adherence_counseling = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/pain_management_na_reason" type="xsd:string" relevant="/data/pain_management = 'Not Availed' or /data/pain_management = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/pmtct_na_reason" type="xsd:string" relevant="/data/pmtct = 'Not Availed' or /data/pmtct = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/condoms_na_reason" type="xsd:string" relevant="/data/condoms = 'Not Availed' or /data/condoms = 'Referred'"/>
			<bind nodeset="/data/not_availed_reasons/other_health_service_na_reason" type="xsd:string" relevant="/data/other_health_services = 'Not Availed' or /data/other_health_services = 'Referred'"/>
			<bind nodeset="/data/form_type" type="xsd:string"/>
			<bind nodeset="/data/new_diagnosis" type="xsd:string"/>
			<itext>
				<translation lang="en" default="">
					<text id="new_diagnosis_na_reason-label">
						<value>New diagnosis / pre-ART enrollment, CD4 test</value>
					</text>
					<text id="end-of-life_hospice_na_reason-label">
						<value>End-of-life hospice/hospital admission</value>
					</text>
					<text id="hiv_counseling_na_reason-label">
						<value>HIV counseling and testing</value>
					</text>
					<text id="family_planning_na_reason-label">
						<value>Family Planning</value>
					</text>
					<text id="sexually_transmitted_na_reason-label">
						<value>Sexually transmitted infection</value>
					</text>
					<text id="art_adherence_na_reason-label">
						<value>ART adherence counseling</value>
					</text>
					<text id="not_availed_reasons-label">
						<value>Not Availed Reasons</value>
					</text>
					<text id="pain_management_na_reason-label">
						<value>Pain Management</value>
					</text>
					<text id="pmtct_na_reason-label">
						<value>PMTCT</value>
					</text>
					<text id="other_health_service_na_reason-label">
						<value>Other Health Service or health education</value>
					</text>
					<text id="condoms_na_reason-label">
						<value>Condoms</value>
					</text>
				</translation>
			</itext>
		</model>
	</h:head>
	<h:body>
		<group ref="/data/not_availed_reasons">
			<label ref="jr:itext('not_availed_reasons-label')"/>
			<input ref="/data/not_availed_reasons/new_diagnosis_na_reason">
				<label ref="jr:itext('new_diagnosis_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/end-of-life_hospice_na_reason">
				<label ref="jr:itext('end-of-life_hospice_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/hiv_counseling_na_reason">
				<label ref="jr:itext('hiv_counseling_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/family_planning_na_reason">
				<label ref="jr:itext('family_planning_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/sexually_transmitted_na_reason">
				<label ref="jr:itext('sexually_transmitted_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/art_adherence_na_reason">
				<label ref="jr:itext('art_adherence_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/pain_management_na_reason">
				<label ref="jr:itext('pain_management_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/pmtct_na_reason">
				<label ref="jr:itext('pmtct_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/condoms_na_reason">
				<label ref="jr:itext('condoms_na_reason-label')"/>
			</input>
			<input ref="/data/not_availed_reasons/other_health_service_na_reason">
				<label ref="jr:itext('other_health_service_na_reason-label')"/>
			</input>
		</group>
	</h:body>
</h:html>